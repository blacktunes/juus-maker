{
  "name": "juus-maker",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite --port 8080 --host",
    "dev:talk": "cross-env VITE_BUILD_MODE=TALK vite --port 8080 --host",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:talk": "cross-env VITE_BUILD_MODE=TALK run-p build",
    "preview": "vite preview",
    "build-only": "vite build",
    "build-only:talk": "cross-env VITE_BUILD_MODE=TALK vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@marshallswain/vuedraggable": "^4.2.0",
    "mitt": "^3.0.1",
    "star-rail-vue": "^1.2.0",
    "super-image-cropper": "^1.0.18",
    "vue": "^3.4.27",
    "vue-picture-cropper": "^0.7.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.3",
    "@tsconfig/node18": "^18.2.4",
    "@types/node": "^18.19.34",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.26.0",
    "fs-extra": "^11.2.0",
    "npm-run-all2": "^6.2.0",
    "prettier": "^3.3.1",
    "prettier-plugin-stylus-format": "^1.0.1",
    "puppeteer": "^22.10.0",
    "stylus": "^0.63.0",
    "typescript": "^5.4.5",
    "unplugin-auto-import": "^0.17.6",
    "vite": "^5.2.12",
    "vite-plugin-pwa": "^0.19.8",
    "vite-plugin-vue-devtools": "^7.2.1",
    "vue-tsc": "^2.0.19"
  }
}